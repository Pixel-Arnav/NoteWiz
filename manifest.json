{
    "manifest_version": 3,
    "name": "Take Note",
    "version": "1.0",
    "description": "A simple browser extension to take and save personal notes  while browsing the websites by Arnav",
    "permissions": ["storage"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      }
    },
    "icons": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  }
  